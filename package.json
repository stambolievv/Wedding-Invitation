{
  "name": "wedding-invitation",
  "version": "1.0.0",
  "description": "Beautiful Custom Paperless Wedding Invitations.",
  "main": "index.html",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite serve",
    "build": "vite build && npm run imageCompression && npm run jsonCompression",
    "preview": "npm run build && vite preview",
    "format": "prettier --write ./src/**",
    "imageCompression": "imagemin dist/assets/images/* --out-dir=dist/assets/images",
    "jsonCompression": "minify-json dist/assets/json",
    "gh-deploy": "npm run gh-predeploy && gh-pages --dist=\"dist\" --branch=\"gh-pages\" --message=\"fix: deploy dist for github pages\"",
    "gh-predeploy": "git rev-parse --verify --quiet gh-pages || (git branch gh-pages && git push --set-upstream origin gh-pages)"
  },
  "author": "Eleonora Atanasova & Deyan Stamboliev",
  "license": "Apache-2.0",
  "keywords": [
    "wedding",
    "invitation",
    "paperless",
    "custom"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stambolievv/Wedding-Invitation.git"
  },
  "bugs": {
    "url": "https://github.com/stambolievv/Wedding-Invitation/issues"
  },
  "dependencies": {
    "lit": "^2.6.1",
    "page": "^1.11.6"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.2",
    "gh-pages": "^5.0.0",
    "imagemin-cli": "^7.0.0",
    "minify-json": "^1.0.0",
    "prettier": "^2.8.4",
    "vite": "^3.2.5",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-static-copy": "^0.13.1"
  }
}
